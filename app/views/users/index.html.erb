<h1>Users</h1>

<%= link_to("Add new user",new_user_path) %>

<table>
  <thead>
    <th>Name</th>
    <th>Role</th>
    <th>Email</th>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.full_name %> </td>
      <td><%= user.role_name %> </td>
      <td><%= user.email %> </td>
      <td><%= link_to "Edit", edit_user_path(user) %> </td>
    </tr>
  <% end %>
</table>